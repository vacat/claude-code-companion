          {
            "expr": "rate(claude_proxy_endpoint_requests_success[5m]) / rate(claude_proxy_endpoint_requests_total[5m]) * 100",
            "legendFormat": "{{endpoint}} 成功率 (%)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
