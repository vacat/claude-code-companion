        "id": 1,
        "title": "端点状态概览",
        "type": "stat",
        "targets": [
          {
            "expr": "claude_proxy_endpoints_total",
            "legendFormat": "总端点数",
            "refId": "A"
          },
          {
            "expr": "claude_proxy_endpoints_active",
            "legendFormat": "活跃端点数",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "端点请求总数",
        "type": "timeseries",
